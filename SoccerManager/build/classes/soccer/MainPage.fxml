            <Button fx:id="clubButton" layoutX="45.0" layoutY="53.0" mnemonicParsing="false" onAction="#club" prefHeight="250.0" prefWidth="350.0" style="-fx-background-color: rgba(110, 86, 222,0.95);" stylesheets="@buttonhover.css" text="Club" textFill="WHITE">
               <font>
                  <Font size="48.0" />
               </font>
            </Button>
